	</bean>
	<bean id="exameRepository"
		class="br.com.eclinic.hibernate.exame.ExameHibernateDAO">
		<constructor-arg ref="sessionFactory" />
	</bean>
	<bean id="planoRepository"
		class="br.com.eclinic.hibernate.plano.PlanoHibernateDAO">
		<constructor-arg ref="sessionFactory" />
	</bean>
	<bean id="eventoRepository"
		class="br.com.eclinic.hibernate.evento.EventoHibernateDAO">
		<constructor-arg ref="sessionFactory" />
	</bean>
	<bean id="empresaRepository"
		class="br.com.eclinic.hibernate.empresa.EmpresaHibernateDAO">
		<constructor-arg ref="sessionFactory" />
	</bean>
